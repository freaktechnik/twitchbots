Header set Strict-Transport-Security "max-age=10886400; includeSubDomains; preload"
Header set Content-Security-Policy "default-src 'self'; style-src 'self' 'unsafe-inline'; frame-ancestors 'none'; connect-src https://api.twitchbots.info"

SetEnv MODE=development
SetEnvIf HTTP_HOST ^(www\.)?twitchbots.info$ MODE=production

RewriteEngine On

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^ index.php [QSA,L]
